                android:layout_margin="10dp"
                android:gravity="center_vertical"
                android:text="RGB"
                android:textSize="16sp"
                android:textStyle="bold"
                android:textColor="#888"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear5"
            android:layout_width="match_parent"
